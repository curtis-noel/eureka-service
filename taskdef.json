{
    "family": "eureka-demo",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "eureka-demo",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "rdfs",
                    "awslogs-region": "<region_name>",
                    "awslogs-stream-prefix": "bwce"
                }
	        }
        }
    ]
}
